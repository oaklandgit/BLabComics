{% set groupLabels = ({
    "blab":"B-Lab",
    "misc": "Random",
    "pineapple": "I Am a Pineapple",
    "patent": "Patent Pending"
}) %}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Larry Stone's Comic Strips">
    <meta name="keywords" content="B-Lab,political humor,tech humor,comics,whimsical,zeno,Dijon the Armadillo,U-Gene,I Am a Pineapple">
    <!-- FONT AWESOME -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">

    <link rel="stylesheet" href="/static/styles.css">
    
    <title>Larry Stone - {% block title %}{% endblock %}</title>
</head>
<body>
    <div id="container">
    <header>
    <span id="logo"><a href="/">Larry Stone</a></span>
    <nav>
    <ul>
    {% for href, caption in [('/comics', 'Comics'), ('/game', 'Game'), ('/about', 'Me')] %}
        <li><a href="{{ href }}"> &bull; {{ caption }}</a></li>
    {% endfor %}
    </ul>
    </nav>
    </header>
    <section>
    {% block content %}{% endblock %}
    </section>
    <footer>&copy 2022 Larry Stone</footer>
    </div>
</body>
</html>