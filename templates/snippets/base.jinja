{% set groupLabels = ({
    "blab":"From the Vault",
    "misc": "More Random Nonsense",
    "pineapple": "I Am a Pineapple",
    "patent": "Patent Pending"
}) %}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{ pageDescription }}">
    <meta name="keywords" content="Web comic, {{ keywords | default("Webcomic, comic strip") }}">
    <meta property="og:title" content="B-Lab: {{ pageTitle }}" />
    <meta property="og:image" content="https://blabcomic.com/static/full2x/{{ metaImage }}" />
    <!-- FONT AWESOME -->
{#     <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css"> #}
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/brands.min.css">


    <link rel="stylesheet" href="/static/styles.css">
    
    <title>B-Lab: {{ pageTitle }}</title>
</head>
<body>
    <div id="container">
    <header>
    <span id="logo"><a href="/">B-Lab</a></span>
    <nav>
    <ul>
    {% set current = request.path.split('/') %}
    {% for href, caption in [('comics', 'Comics'), ('about', 'About')] %}
        <li{% if href == current[1] %} class="current" {% endif %}><a href="/{{ href }}">{{ caption }}</a></li>
    {% endfor %}
    </ul>
    </nav>
    </header>
    <section>
    {% block content %}{% endblock %}
    </section>
    <footer>

&copy 2022 Larry Stone

<a href="http://twitter.com/elstono/media"><i class="fa-brands fa-twitter fa-sm"></i></a>
<a href="https://www.linkedin.com/in/larrystone/"><i class="fa-brands fa-linkedin fa-sm"></i></a>


</footer>
    </div>
</body>
</html>